{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.5.2"
        }
    },
    "rewrites": [
        {
            "source": "/database.sqlite",
            "destination": "https://php-one.vercel.app/database.sqlite"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ]
}